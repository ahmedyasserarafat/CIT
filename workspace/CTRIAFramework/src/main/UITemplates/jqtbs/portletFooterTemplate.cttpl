				<a data-item-id={{FLD_BBAR_BTN_ID}} data-item-type="BBAR_left_button" system_btn_ind="{{FLD_IS_SYS_BUTTON}}" class="{{BBAR_CLASS}} ct_btn">{{FLD_BTN_DISPLAY_NM}}</a>
			</div>
			</div>
			</li>
			{{/each}}
			<li>
			{{#each RIGHT_BBAR_BUTTONS}}
