			javaType="java.lang.String" jdbcType="VARCHAR" />
		<result property="sourceValue" nullValue="" column="SOURCE_VALUE"
			javaType="java.lang.String" jdbcType="VARCHAR" />
		<result property="visibleInd" nullValue="" column="VISIBLE_IND"
			javaType="java.lang.String" jdbcType="VARCHAR" />
	</resultMap>

