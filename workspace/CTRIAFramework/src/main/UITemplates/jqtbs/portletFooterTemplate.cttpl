				<a data-item-id={{FLD_BBAR_BTN_ID}} data-item-type="BBAR_right_button" system_btn_ind="{{FLD_IS_SYS_BUTTON}}" class="{{BBAR_CLASS}} ct_btn_neg">{{FLD_BTN_DISPLAY_NM}}</a>
			</div>
		</div>
		{{/each}}
		</li>
			</ul>
		</div>
	</div>	
   </div>